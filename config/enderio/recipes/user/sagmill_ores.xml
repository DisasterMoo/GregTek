<?xml version="1.0" encoding="UTF-8"?>
<enderio:recipes xmlns:enderio="http://enderio.com/recipes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://enderio.com/recipes recipes.xsd ">
  <!-- Draconium (Draconic) -->

  <recipe name="Sagmill: Draconium Ore" required="false">
    <sagmilling energy="3600">
      <input name="oreDraconium" />
      <output name="dustDraconium" amount="2" />
      <output name="dustIridium" amount="1" chance="0.25" />
      <output name="minecraft:cobblestone" chance="0.15" />
    </sagmilling>
  </recipe>
  
